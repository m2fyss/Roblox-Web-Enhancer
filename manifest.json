{
    "name": "RobloxWeb+",
    "description": "This extension is work in progress and may have bugs.",
    "version": "1.0.5",
    "manifest_version": 3,

    "action" : {
        "default_icon": "assets/icon.png"
    },

    "host_permissions": [
	"https://*/*"
    ],

    "permissions": [
        "cookies"
    ],

    "background": {
        "service_worker": "scripts/webjs.js"
    },

    "icons": {
        "16": "assets/icon.png",
        "32": "assets/icon.png",
        "48": "assets/icon.png",
        "128": "assets/icon.png"
    }
}